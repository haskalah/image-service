{
  "name": "image-service",
  "version": "0.0.6",
  "description": "Image management service with API key authentication",
  "author": "Haskalah",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "version-update": "npm version patch -f --no-git-tag-version && git add package.json package-lock.json",
    "key:create": "npx ts-node src/scripts/create-api-key.ts",
    "key:list": "npx ts-node src/scripts/list-api-keys.ts",
    "key:update": "npx ts-node src/scripts/update-api-key.ts",
    "key:revoke": "npx ts-node src/scripts/revoke-api-key.ts",
    "migrate:primordia": "npx ts-node ../primordia-backend/scripts/migrate-primordia-images.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@types/mime-types": "^3.0.1",
    "dotenv": "^16.0.1",
    "imagelib": "npm:@haskalah/imagelib@latest",
    "mime-types": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "pre-commit": "^1.2.2",
    "typescript": "^5.1.3"
  },
  "pre-commit": [
    "version-update"
  ]
}
